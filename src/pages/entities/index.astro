---
export const prerender = false

import Layout from '../../components/Layout.astro';
import { Card } from '@astrojs/starlight/components';

type Resource = {
	id: string
}

import type { PageSiteGraphFrontmatter } from 'starlight-site-graph/schema';
import { fetchResourceAt } from '@/scripts/fetchSolidResource';
const frontmatter : PageSiteGraphFrontmatter = {
   title: "entities",
   tags: [],
   sitemap: {linkInclusionRules: ["**/*"]},
   graph: {visible: true},
   backlinks: {visible: true}
 }
---

<Layout pageTitle='entities' pageStyle='leftside' frontmatter={{frontmatter}} starlightSettings={{next: false, prev: false}}>

{await fetchResourceAt('/fragments/autoethnography')
  .then((data) => <Card title="title">{data}</Card>)
  .catch((error) => <Card title="Error">{error.message}</Card>)
}

</Layout>