---

import Layout from '../components/Layout.astro';
import { Card, LinkCard, CardGrid } from '@astrojs/starlight/components';
import { getCollection } from 'astro:content';
const allItems = await getCollection('earth');

---

<Layout pageTitle='in the wind and the wires' tabTitle='index' pageStyle='full' starlightSettings={{next: false, prev: false, graph: {visible: false}}}>

<CardGrid>
      <LinkCard title={'earth'} href={'/earth/start-here'} />
      <LinkCard title={'library'} href={'/library/start-here'} />
      <LinkCard title={'mulch'} href={'/mulch'} />
      <LinkCard title={'vignettes'} href={'/vignettes'} />
 </CardGrid>
</Layout>